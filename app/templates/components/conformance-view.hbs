          </div>
        </div>
      </div>
      <!-- END DOCUMENT METADATA  -->
      <!-- START MESSAGING METADATA  -->
      <div id="messaging-metadata" class="tab-pane">
        <div class="panel-body">
          <div class="row">
            <div class="col-md-4">
              <div><label>MSG:</label></div>
            </div>
            <div class="col-md-8">

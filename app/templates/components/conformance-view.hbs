    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
      <div class="panel-title table-header row">
        <div class="col-md-3">
          <span class="table-cell">{{r.mode}}</span>
        </div>
        <div class="col-md-9">
          <div class="col-md-1"><i {{bind-attr class=":glyphicon :test-circle r.operation.transaction:filled :conformance-test-badge r.results.transaction.status"}}>{{#if r.results.transaction.status}}{{#bootstrap-popover title="'''''" tagName="span"}}{{render 'partials/_conformancePopover' r.results.transaction}}{{/bootstrap-popover}}{{/if}}</i></div><div class="col-md-3">Transaction</div>

<div class="col-md-12 tab-content-holder">
  <!-- BEGIN RESOURCE CHART -->
  <div class="row conformance">
    {{!-- <div id="resource-chart" class="col-md-12"> --}}
    {{!-- {{#each conformance.rest as |r|}} --}}
    <div id="resource-chart" class="table-responsive">
      <table class="col-md-12 table table-striped table-condensed">
        <thead>
          <tr>
            <th>Resource Type</th>
            <th>Read</th>
            <th>VRead</th>
            <th>Update</th>
            <th>Delete</th>
            <th>History</th>
            <th>Validate</th>
            <th>History Type</th>
            <th>Create</th>
            <th>Search Type</th>
          </tr>
        </thead>
        <tbody>
          {{#each conformance.rest as |r|}}
            {{#each r.resource as |res|}}
              <tr>
                <td data-title="Read" class="test-circle-holder">
                  <a class="collapsed" data-toggle="collapse" data-parent="#accordion" aria-expanded="false" data-target="#resource{{unbound res.fhirType}}">{{title-case res.fhirType}}</a>
                </td>
                {{#each actions as |operation|}}
                  <td>
                    {{resource-operation resource=res validatedResources=validatedResources operation=operation}}
                  </td>
                {{/each}}
              </tr>
              <tr>
                <td colspan="10">
                  <div id="resource{{unbound res.fhirType}}" class="panel-collapse collapse">
                    Conforms to
                    <a {{bind-attr href="{{res.profile.reference}}"}}>{{res.profile.reference}}</a>
                    <table class="table">
                      <tr>
                        <th>Search Param</th>
                        <th>Type</th>
                        <th>Description</th>
                      </tr>
                      {{#each res.searchParam as |param|}}
                        <tr>
                          <td>{{param.name}}</td>
                          <td>{{param.fhirType}}</td>
                          <td>{{param.documentation}}</td>
                        </tr>
                      {{/each}}
                    </table>
                  </div>
                </td>
              </tr>
            {{/each}}
          {{/each}}
        </tbody>
      </table>
    </div>




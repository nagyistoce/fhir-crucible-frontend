
<div class="container main">
  <div class="row content">
    <div class="col-md-12">
      <div class="palette report">
        <div class="row">
          <div class="col-md-10">
            <h1>
              {{model.server.name}}<br>
              <small>{{model.server.url}}</small>
              <div class="pull-right">
                <small><span class="server-time"><i class="fa fa-fw fa-clock-o"/> {{model.generatedFromNow}}</span></small>
              </div>
            </h1>

          </div>
          <div class="col-md-2">
            <button class="btn" {{action "executeTests"}}>Execute Tests</button>
          </div>
        </div>
        {{server-fhir-specification data=model}}
      </div>
    </div>
  </div>
</div>
